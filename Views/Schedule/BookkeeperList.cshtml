@model BookKeeperListModel
<header classs="jumbotron text-center">
    <img src="~/images/Logo.png" class="img-fluid center-block" />
</header>

<div class="row">
    @foreach (BookKeeper bookKeeper in Model.Bookkeepers)
    {
        <div class="col-md-3">
            <div class="img-thumbnail mt-3 thumbnails">
                <img class="imgprofile" src="~/images/profilepic.png" />
                <div class="figure-caption text-center">
                    <h5>@bookKeeper.FirstName @bookKeeper.LastName</h5>
                    <h6>@bookKeeper.Description</h6>
                    <button class="btn btn-primary">Hire!</button>
                    <hr />
                    <h6 class="text-right">@bookKeeper.PayRate.ToString("c")/hr</h6>
                </div>
            </div>
        </div>
    }
    </div>


